{
    "name": "goopil/eloquent-rest-filter",
    "description": "set of eloquent scopes to implements various rest query strings",
    "keywords": [
        "laravel",
        "database",
        "db",
        "api",
        "filter",
        "eloquent"
    ],
    "homepage": "https://gitlab.com/goopil/lib/laravel/rest-filter",
    "support": {
        "issues": "https://gitlab.com/goopil/lib/laravel/rest-filter/issues"
    },
    "version": "0.2.2",
    "license": "MIT",
    "authors": [{
        "name": "Goopil",
        "email": "me@goopil.org"
    }],
    "require": {
        "php": ">=5.6",
        "illuminate/container": ">=5.4.0",
        "illuminate/contracts": ">=5.4.0",
        "illuminate/database": ">=5.4.0",
        "illuminate/support": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.3",
        "orchestra/testbench":  ">=3.4",
        "orchestra/database": ">=3.4"
    },
    "autoload": {
        "psr-4": {
            "Goopil\\RestFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goopil\\RestFilter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "win-test": "vendor\\bin\\phpunit --debug --coverage-text",
        "test": "vendor/bin/phpunit --debug --coverage-text --colors=never --bootstrap=./vendor/autoload.php"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goopil\\RestFilter\\RestScopeProvider"
            ]
        }
    }
}
